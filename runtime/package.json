{
  "name": "@genqlx/runtime",
  "version": "3.1.12",
  "description": "Genqlx runtime client",
  "keywords": [],
  "homepage": "https://genqlx.vercel.app",
  "repository": {
    "url": "https://github.com/meabed/genqlx"
  },
  "license": "ISC",
  "author": "Meabed <mo@meabed.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist/*",
    "/esm/*",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist esm *.tsbuildinfo && tsc && tsc -m ESNext --outDir esm",
    "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
    "start": "tsdx watch",
    "test": "yarn typesTests && jest",
    "typesTests": "tsc -p src/client/__tests__/tsconfig.json",
    "watch": "tsc -w"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "mocha": {
    "require": "tests/init.js",
    "spec": "tests/**.ts",
    "timeout": 9999999999
  },
  "dependencies": {
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "graphql-ws": "^5.8.2",
    "is-plain-obj": "^4.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash.assign": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.startswith": "^4.2.1",
    "ws": "^8.6.0",
    "zen-observable-ts": "^1.1.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "2.2.1",
    "@types/common-tags": "1.8.1",
    "@types/jest": "27.5.1",
    "@types/lodash": "4.14.182",
    "@types/lodash.assign": "4.2.7",
    "@types/lodash.get": "4.4.7",
    "@types/lodash.startswith": "4.2.7",
    "@types/node": "17.0.35",
    "@types/qs": "6.9.7",
    "@types/ws": "8.5.3",
    "common-tags": "1.8.2",
    "dotenv": "16.0.1",
    "jest": "28.1.0",
    "sucrase": "3.21.0",
    "ts-jest": "28.0.2",
    "tslib": "2.4.0",
    "utility-types": "3.10.0"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "_": "[bump]"
}
