{
  "name": "@genqlx/runtime",
  "version": "3.1.0",
  "description": "Genqlx runtime client",
  "keywords": [],
  "homepage": "https://genqlx.vercel.app",
  "repository": {
    "url": "https://github.com/meabed/genqlx"
  },
  "license": "ISC",
  "author": "Meabed <mo@meabed.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist/*",
    "/esm/*",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist esm *.tsbuildinfo && cp ./package.json src/version.json && tsc && tsc -m ESNext --outDir esm",
    "compile": "sucrase -q ./src -d ./dist --transforms typescript,imports",
    "dev": "yarn compile && node dist",
    "start": "tsdx watch",
    "test": "yarn typesTests && jest",
    "typesTests": "tsc -b src/client/__tests__/tsconfig.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/_old/**",
      "!**/*.case.ts",
      "!src/testHelpers/**/*.ts"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "mocha": {
    "require": "tests/init.js",
    "spec": "tests/**.ts",
    "timeout": 9999999999
  },
  "dependencies": {
    "graphql-query-batcher": "^1.0.1",
    "graphql-ws": "^5.8.1",
    "isomorphic-unfetch": "^3.1.0",
    "isomorphic-ws": "^4.0.1",
    "lodash.assign": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.startswith": "^4.2.1",
    "ws": "^8.6.0",
    "zen-observable-ts": "^1.1.0"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.2.1",
    "@types/common-tags": "^1.8.1",
    "@types/jest": "^27.5.0",
    "@types/listr": "^0.14.4",
    "@types/lodash": "^4.14.182",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.31",
    "@types/qs": "^6.9.7",
    "@types/rimraf": "^3.0.2",
    "@types/ws": "^8.5.3",
    "common-tags": "^1.8.2",
    "dotenv": "^16.0.0",
    "jest": "^28",
    "sucrase": "^3.21.0",
    "ts-jest": "^27.1.4",
    "tslib": "^2.4.0",
    "utility-types": "^3.10.0"
  },
  "_": "[bump]"
}
